{
  "manifest_version": 2,
  "name": "Cohesion Redirector",
  "version": "1.2",
  "description": "Redirects Notion URLs to the Cohesion desktop app.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "48": "icon.png"
  }
}
